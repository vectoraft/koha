[%- USE raw -%]
[%- USE Koha -%]
[%- SET delimiter = Koha.CSVDelimiter() -%]

[%- INCLUDE csv_headers/acqui/basket.tt -%]
[%- INCLUDE empty_line.inc -%]

[%- FOREACH r IN rows -%]
    [%- '"' | $raw %][% r.contractname | html %][% '"' | $raw %][%- delimiter | html -%]
    [%- r.ordernumber | html %][%- delimiter | html -%]
    [%- r.entrydate | html %][%- delimiter | html -%]
    [%- r.isbn | html %][%- delimiter | html -%]
    [%- '"' | $raw %][% r.author | html %][% '"' | $raw %][%- delimiter | html -%][% '"' | $raw %][% r.title | html %][% '"' | $raw %][%- delimiter | html -%]
    [%- r.publicationyear | html %][%- delimiter | html -%]
    [%- '"' | $raw %][% r.publishercode | html %][% '"' | $raw %][%- delimiter | html -%][% '"' | $raw %][% r.collectiontitle | html %][% '"' | $raw %][%- delimiter | html -%][% '"' | $raw %][% r.notes | html %][% '"' | $raw %][%- delimiter | html -%]
    [%- r.quantity | html %][%- delimiter | html -%]
    [%- r.rrp | html %][%- delimiter | html -%]
    [%- '"' | $raw %][% r.deliveryplace | html %][% '"' | $raw %][%- delimiter | html -%][% '"' | $raw %][% r.billingplace | html %][% '"' | $raw %][%- INCLUDE empty_line.inc -%]
[%- END -%]
