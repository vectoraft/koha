[%- USE raw -%]
[%- USE Koha -%]
[%- SET delimiter = Koha.CSVDelimiter() -%]

[%- USE Price -%]
[%- INCLUDE csv_headers/acqui/basketgroup.tt -%]
[%- INCLUDE empty_line.inc -%]

[%- FOREACH r IN rows -%]
    [%- r.clientnumber | html %][%- delimiter | html -%]
    [%- '"' | $raw %][% r.basketname | html %][% '"' | $raw %][%- delimiter | html -%]
    [%- r.ordernumber | html %][%- delimiter | html -%]
    [%- '"' | $raw %][% r.author | html %][% '"' | $raw %][%- delimiter | html -%][% '"' | $raw %][% r.title | html %][% '"' | $raw %][%- delimiter | html -%][% '"' | $raw %][% r.publishercode | html %][% '"' | $raw %][%- delimiter | html -%]
    [%- r.publicationyear | html %][%- delimiter | html -%]
    [%- '"' | $raw %][% r.collectiontitle | html %][% '"' | $raw %][%- delimiter | html -%]
    [%- r.isbn | html %][%- delimiter | html -%]
    [%- r.quantity | html %][%- delimiter | html -%]
    [%- r.rrp_tax_included | $Price %][%- delimiter | html -%]
    [%- r.rrp_tax_excluded | $Price %][%- delimiter | html -%]
    [%- r.discount | html %][%- delimiter | html -%]
    [%- r.ecost_tax_included | $Price %][%- delimiter | html -%]
    [%- r.ecost_tax_excluded | $Price %][%- delimiter | html -%]
    [%- '"' | $raw %][% r.notes | html %][% '"' | $raw %][%- delimiter | html -%]
    [%- r.entrydate | html %][%- delimiter | html -%]
    [%- '"' | $raw %][% r.booksellername | html %][% '"' | $raw %][%- delimiter | html -%][% '"' | $raw %][% r.bookselleraddress | html %][% '"' | $raw %][%- delimiter | html -%][% '"' | $raw %][% r.booksellerpostal | html %][% '"' | $raw %][%- delimiter | html -%]
    [%- r.contractnumber | html %][%- delimiter | html -%]
    [%- '"' | $raw %][% r.contractname | html %][% '"' | $raw %][%- delimiter | html -%][% '"' | $raw %][% r.basketgroupdeliveryplace | html %][% '"' | $raw %][%- delimiter | html -%][% '"' | $raw %][% r.basketgroupbillingplace | html %][% '"' | $raw %][%- delimiter | html -%]
    [%- '"' | $raw %][% r.basketdeliveryplace | html %][% '"' | $raw %][%- delimiter | html -%][% '"' | $raw %][% r.basketbillingplace | html %]"[%- INCLUDE empty_line.inc -%]
[%- END -%]
